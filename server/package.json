{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "cross-env NODE_ENV=production node dist/bin/www.js",
    "dev": "cross-env NODE_ENV=development tsc-watch  --onSuccess 'node --trace-warnings --enable-source-maps ./dist/bin/www.js'",
    "format": "prettier --write .",
    "test": "jest --passWithNoTests"
  },
  "engines": {
    "node": ">=16.x.x",
    "yarn": ">=1.x.x"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.4",
    "peer": "^0.6.1",
    "qrcode": "^1.5.0",
    "sequelize": "^6.21.3",
    "socket.io": "^4.4.1",
    "sqlite3": "^5.0.8"
  },
  "author": {
    "name": "Micah Effiong",
    "email": "micaiah.effiong@gmail.com"
  },
  "contributors": [
    {
      "name": "Micah Effiong",
      "email": "micaiah.effiong@gmail.com"
    }
  ],
  "nodemonConfig": {
    "ignore": [
      "public"
    ]
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/qrcode": "^1.4.2",
    "jest": "^28.1.3",
    "nodemon": "^2.0.13",
    "ts-jest": "^28.0.7",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.6.2"
  }
}
