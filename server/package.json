{
  "name": "file-share",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/bin/www.js",
    "serve": "cross-env NODE_ENV=development node dist/bin/www.js",
    "start:dev": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon dist/bin/www.js\"",
    "serve:dev": "cp -r src/public dist/public/ && cp -r src/views dist/views && cross-env NODE_ENV=development tsc-watch --onSuccess \"node dist/bin/www.js\""
  },
  "engines": {
    "node": "^14.18.0",
    "yarn": "^1.22.17"
  },
  "dependencies": {
    "concurrently": "^7.0.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.4",
    "peer": "^0.6.1",
    "qrcode": "^1.5.0",
    "socket.io": "^4.4.1"
  },
  "author": {
    "name": "Micah Effiong",
    "email": "micaiah.effiong@gmail.com"
  },
  "contributors": [
    {
      "name": "Micah Effiong",
      "email": "micaiah.effiong@gmail.com"
    }
  ],
  "nodemonConfig": {
    "ignore": [
      "public"
    ]
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/qrcode": "^1.4.2",
    "nodemon": "^2.0.13",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.6.2"
  }
}
